<%- include('partials/header'); -%>
<h1>Compose</h1>

<form class="" action="/compose" method="post">
<div class="mb-3">
    <label class="form-label">Title</label>
    <input name="postTitle" type="text" class="form-control" id="exampleFormControlInput1" >
  </div>
  <div class="mb-3">
    <label class="form-label">Post</label>
    <textarea name="postBody" class="form-control" rows="5"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer'); -%>
